{
  "name": "graphql-schema-registry",
  "version": "1.1.3",
  "description": "Graphql schema registry",
  "main": "schema-registry.js",
  "scripts": {
    "test": "./node_modules/.bin/jest --config test/unit/jest.config.js",
    "coverage": "./node_modules/.bin/jest --config test/unit/jest.coverage.js && coveralls < coverage/lcov.info",
    "test-travis": "npm run test",
    "format": "prettier --write '**/*.{js,jsx,json,md,yml,yaml,graphql}'",
    "version": "version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md",
    "build": "rimraf dist && mkdirp dist && webpack --env.production --bail",
    "docker-compose-validate": "docker-comply compose --file ./docker-compose.deploy.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pipedrive/graphql-schema-registry.git"
  },
  "keywords": [
    "graphql"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run format",
      "pre-push": "npm run format"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@apollo/client": "^3.3.4",
    "@apollo/federation": "^0.20.7",
    "@graphql-inspector/core": "^2.3.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "apollo-server-express": "^2.19.0",
    "async-redis": "^1.1.7",
    "body-parser": "^1.19.0",
    "dataloader": "^2.0.0",
    "date-fns": "^2.9.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "graphql-voyager": "^1.0.0-rc.31",
    "joi": "^17.2.1",
    "knex": "^0.20.2",
    "lodash": "^4.17.20",
    "mysql2": "^2.0.1",
    "object-hash": "^2.0.1",
    "prismjs": "^1.21.0",
    "react-diff-viewer": "^3.1.1",
    "react-router-dom": "^5.1.2",
    "request-promise": "^4.2.5",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-prismjs": "^1.1.1",
    "babel-plugin-styled-components": "^1.10.7",
    "chai": "^4.2.0",
    "changelog-verify": "^1.1.2",
    "commander": "^6.0.0",
    "coveralls": "^3.1.0",
    "css-loader": "^3.4.0",
    "eslint": "^6.6.0",
    "eslint-config-pipedrive": "^5.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "file-loader": "^5.0.2",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^6.2.3",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "2.1.1",
    "prop-types": "^15.7.2",
    "proxyquire": "^2.1.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-hot-loader": "^4.12.18",
    "recursive-readdir": "^2.2.2",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "sinon": "^8.0.1",
    "terser-webpack-plugin": "^2.3.8",
    "uid": "0.0.2",
    "version-changelog": "^3.1.1",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  },
  "pre-push": [],
  "post-merge": []
}
