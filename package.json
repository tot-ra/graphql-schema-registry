{
  "name": "graphql-schema-registry",
  "version": "3.1.0",
  "description": "Graphql schema registry",
  "main": "app/schema-registry.js",
  "scripts": {
    "develop": "DB_HOST=localhost DB_PORT=6000 REDIS_HOST=localhost REDIS_PORT=6004 PORT=6001 NODE_ENV=development ts-node-dev --respawn --transpile-only src/schema-registry.ts --watch src --inspect",
    "test": "./node_modules/.bin/jest --config test/unit/jest.config.cjs",
    "coverage": "./node_modules/.bin/jest --config test/unit/jest.coverage.cjs && coveralls < coverage/lcov.info",
    "test-travis": "npm run test",
    "test-functional": "./node_modules/.bin/jest --config test/functional/jest.config.cjs",
    "format": "prettier --write '**/*.{js,jsx,json,md,yml,yaml,graphql}'",
    "version": "version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md",
    "build": "npm run build-backend && npm run build-frontend",
    "build-backend": "rimraf app && mkdirp app && tsc",
    "build-frontend": "webpack --node-env production --bail --config webpack.config.js",
    "docker-compose-validate": "docker-comply compose --file ./docker-compose.deploy.yml",
    "migrate-db": "knex migrate:latest",
    "new-db-migration": "knex migrate:make --stub ./migrations/sql.stub rename_me",
    "list-db-migrations": "knex migrate:list",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx client src",
    "lint:fix": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx client src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pipedrive/graphql-schema-registry.git"
  },
  "keywords": [
    "graphql"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run format",
      "pre-push": "npm run format"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@apollo/client": "3.5.8",
    "@apollo/federation": "0.34.0",
    "@graphql-inspector/core": "2.9.0",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "apollo-server-express": "2.25.3",
    "async-redis": "1.1.7",
    "body-parser": "1.19.1",
    "dataloader": "2.0.0",
    "date-fns": "2.28.0",
    "dotenv": "10.0.0",
    "express": "4.17.2",
    "graphql": "15.5.3",
    "joi": "17.6.0",
    "kafkajs": "1.15.0",
    "knex": "0.20.2",
    "lodash": "4.17.21",
    "mysql2": "1.7.0",
    "prismjs": "1.26.0",
    "react": "^17.0.2",
    "react-diff-viewer": "3.1.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "5.3.0",
    "styled-components": "5.3.3",
    "winston": "3.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.8.3",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-prismjs": "^2.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "chai": "^4.2.0",
    "changelog-verify": "^1.1.2",
    "commander": "^6.0.0",
    "core-js": "^3.22.3",
    "coveralls": "^3.1.0",
    "css-loader": "^3.4.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.14.0",
    "eslint-config-pipedrive": "^12.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "file-loader": "^5.0.2",
    "husky": "^4.2.5",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.6.0",
    "morgan": "^1.9.1",
    "prettier": "^2.6.2",
    "prop-types": "^15.7.2",
    "proxyquire": "^2.1.3",
    "recursive-readdir": "^2.2.2",
    "request": "^2.88.0",
    "request-promise": "4.2.6",
    "request-promise-native": "^1.0.8",
    "sinon": "^8.0.1",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^2.0.0-0",
    "typescript": "^4.6.4",
    "version-changelog": "^3.1.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "pre-push": [],
  "post-merge": []
}
